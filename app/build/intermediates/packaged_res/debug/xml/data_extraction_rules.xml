<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- This app participates in cloud backup, default is include.
        <include domain="file" path="."/>
        <exclude domain="file" path="no_backup/"/>
        <exclude domain="database" path="test.db"/>
        <exclude domain="sharedpref" path="device_preferences.xml"/>
        -->
    </cloud-backup>
    <device-transfer>
        <!-- 
        <include domain="file" path="."/>
        <exclude domain="file" path="no_backup/"/>
        <exclude domain="database" path="test.db"/>
        <exclude domain="sharedpref" path="device_preferences.xml"/>
        -->
    </device-transfer>
</data-extraction-rules>